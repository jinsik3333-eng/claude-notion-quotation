# Notion API 설정
# Notion 통합(Integration) 토큰: https://www.notion.so/my-integrations
NOTION_API_KEY=secret_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Notion 견적서 데이터베이스 ID
# 데이터베이스 URL에서 추출: notion.so/[workspace]/[DATABASE_ID]?v=...
NOTION_QUOTE_DATABASE_ID=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Notion 견적 항목 데이터베이스 ID
# 데이터베이스 URL에서 추출: notion.so/[workspace]/[DATABASE_ID]?v=...
NOTION_ITEMS_DATABASE_ID=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Google Gemini API 설정
# API 키 생성: https://ai.google.dev/
NEXT_PUBLIC_GOOGLE_GEMINI_API_KEY=AIzaXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# 앱 URL (선택)
NEXT_PUBLIC_APP_URL=http://localhost:3000
